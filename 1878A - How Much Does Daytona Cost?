// You are using GCC
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int x;
    cin>>x;
    while(x--)
    {
        int n;
        cin>>n;
        int target;
        cin>>target;
        int count=0;
        int arr[n];
        for(int i=0;i<n;i++)
        {
            cin>>arr[i];
            if(arr[i]==target)
            count++;
        }
        if(count>=1)
        cout<<"Yes"<<endl;
        else
        cout<<"No"<<endl;
    }
}
