// You are using GCC
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int x;
    cin>>x;
    while(x--)
    {
        int n;
        cin>>n;
        int sum=0;
        int arr[n];
        for(int i=0;i<n-1;i++)
        {
            cin>>arr[i];
            sum=sum+arr[i];
        }
        cout<<sum*-1<<endl;
    }
}
